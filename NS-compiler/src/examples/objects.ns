var fifo = object {

    int head;
    int tail;
    int size;
    arr array;

    #fix syntax!!

    void push = func [int x] {
        array[tail] = x;
        tail = (tail + 1) % size;
    };

    int pop = func [] {
        int ret = array[head];
        head = (head + 1) % size;
        return ret;
    };
};

fifo.push: 5;
fifo.push: 4;


