#include <stdio.h>
#include <stdlib.h>

void prints (char *s) {
    printf("%s\\n",s);
}
void printi (int i) {
    printf("%d\\n",i);
}

int stdini () {
    int i;
    scanf("%d", &i);
    return i;
}

char *stdinline (int size) {
    char *buffer = malloc(size);
    fgets (buffer, size, stdin);
    return buffer;
}
