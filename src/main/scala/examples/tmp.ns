array := {
	base : [int],
	push: (elem:int) => ?$ self.base.push(elem) ?$,
	clear: () => ?$ self.base.clear() ?$,
	override +: (other:array):array => {
		?$ self.base.extend(other.base); ?$
		return self;
    },
};

sum := (l:[int]) => {
	tmp := 0;
	for (e in l) tmp += e;
	return tmp;
};

l := array([1,2,3]);
r := array(5..10);
l.push: 4;
res := l + r;
println: (sum: res.base));

